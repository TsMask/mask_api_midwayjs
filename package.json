{
  "name": "mask_api_midwayjs",
  "private": true,
  "version": "0.1.3",
  "description": "🎉 基于 Midwayjs 的管理系统后端接口服务",
  "author": "TsMask",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/TsMask/mask_api_midwayjs"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start:docker": "NODE_ENV=production pm2-runtime start ./bootstrap.js --name mask_api -i 2",
    "start:pm2": "NODE_ENV=production pm2 start ./bootstrap.js --name mask_api -i 2",
    "start": "NODE_ENV=production node ./bootstrap.js",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c"
  },
  "dependencies": {
    "@dropb/diskinfo": "^3.0.0",
    "@midwayjs/bootstrap": "^3.0.0",
    "@midwayjs/bull": "^3.0.0",
    "@midwayjs/core": "^3.0.0",
    "@midwayjs/cross-domain": "^3.0.0",
    "@midwayjs/decorator": "^3.0.0",
    "@midwayjs/jwt": "^3.0.0",
    "@midwayjs/koa": "^3.0.0",
    "@midwayjs/logger": "^2.0.0",
    "@midwayjs/redis": "^3.0.0",
    "@midwayjs/static-file": "^3.0.0",
    "@midwayjs/typeorm": "^3.0.0",
    "@midwayjs/upload": "^3.0.0",
    "@midwayjs/security": "^3.0.0",
    "@node-rs/bcrypt": "^1.0.0",
    "cron-parser": "^4.0.0",
    "dayjs": "^1.0.0",
    "mini-svg-data-uri": "^1.4.0",
    "mysql2": "^2.0.0",
    "nanoid": "^3.0.0",
    "svg-captcha": "^1.4.0",
    "typeorm": "^0.3.0",
    "ua-parser-js": "^1.0.0",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/mock": "^3.0.0",
    "@types/jest": "^28.0.0",
    "@types/jsonwebtoken": "^8.0.0",
    "@types/koa": "^2.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.0.0",
    "cross-env": "^7.0.0",
    "jest": "^28.0.0",
    "mwts": "^1.0.0",
    "ts-jest": "^28.0.0",
    "typescript": "^4.0.0"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ]
}